<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elecentlife">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ForgotPassActivity"></activity>
        <activity android:name=".NewEventActivity" />
        <activity android:name=".settingsColors" />
        <activity android:name=".settingsNotif" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".WeeklyActivity" />
        <activity android:name=".FriendListActivity" />
        <activity android:name=".DailyActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".TimePickerFragment" />
        <activity android:name=".Notification" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".AlertReceiver"/>
    </application>

</manifest>